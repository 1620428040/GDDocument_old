{
	"describe":"值班管理",
	"bindTable":"oa_duty",
	"primaryFields":"id",
	"handler":"DataMagicInterim",
	"tools":["browse","insert","delete","update","search","refresh"],
	"alias":{
		"title":"duty_name",
		"people":"duty_username",
		"date":"duty_data"
	},
	"fieldList":{
		"id":{
			"title":"编号",
			"type":"Number"
		},
		"duty_name":{
			"title":"值班名称",
			"notNull":true
		},
		"duty_content":{
			"title":"值班内容",
			"notNull":true,
			"type":"LongText"
		},
		"duty_userid":{
			"title":"值班人ID",
			"notNull":true
		},
		"duty_username":{
			"title":"值班人",
			"notNull":true
		},
		"inputuserid":{
			"title":"登记人ID",
			"notNull":true
		},
		"inputusername":{
			"title":"登记人",
			"notNull":true
		},
		"duty_data":{
			"title":"值班日期",
			"type":"DateTime",
			"notNull":true
		},
		"begin_time":{
			"title":"开始时间",
			"type":"Date",
			"notNull":true
		},
		"end_time":{
			"title":"结束时间",
			"type":"Date",
			"notNull":true
		},
		"visitor":{
			"title":"有无访问者",
			"type":"Bool"
		},
		"visitor_name":{
			"title":"访问者姓名",
			"notNull":true
		},
		"file":{
			"title":"有无文件",
			"type":"Bool"
		},
		"file_name":{
			"title":"文件名",
			"notNull":true
		}
	}
}