{
	"describe":"新闻",
	"bindTable":"oa_news",
	"primaryFields":"id",
	"tools":["browse","refresh"],
	"urls":{
	},
	"alias":{
		"title":"title",
		"people":"username",
		"date":"add_time"
	},
	"fieldList":{
		"id":{
			"title":"编号",
			"type":"number"
		},
		"title":{
			"title":"标题",
			"notNull":"1"
		},
		"content":{
			"title":"内容",
			"notNull":"1"
		},
		"type":{
			"title":"新闻类型",
			"type":"selector",
			"valueRange":"1,内部;0,外部",
			"defaultValue":"1",
			"describe":"",
			"notNull":"1",
			"params":{
				"isMulti":false,
				"returnType":"number"
			}
		},
		"add_time":{
			"title":"添加时间",
			"type":"Date",
			"notNull":"1"
		},
		"tag":{
			"title":"发布或未发布",
			"type":"selector",
			"valueRange":"1,发布;0,未发布",
			"defaultValue":"0",
			"describe":"",
			"notNull":"1",
			"params":{
				"isMulti":false,
				"returnType":"number"
			}
		},
		"userid":{
			"title":"发布人id",
			"notNull":"1"
		},
		"username":{
			"title":"发布人姓名",
			"notNull":"1"
		}
	}
}