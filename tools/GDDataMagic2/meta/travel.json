		"dept_id": {
			"title": "部门ID",
			"notNull": false,
			"dataType": "varchar",
			"maxlength": 64
		},
		"dept_name": {
			"title": "部门名称",
			"notNull": false,
			"dataType": "varchar",
			"maxlength": 64
		},
		"triptype": {
			"title": "出差类型",
			"notNull": false,
			"dataType": "varchar",
			"maxlength": 64
		},
