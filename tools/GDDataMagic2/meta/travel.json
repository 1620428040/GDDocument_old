			"dataType": "varchar",
			"maxlength": 512
		},
		"trip_purpose": {
			"title": "出差目的",
			"notNull": true,
			"dataType": "varchar",
			"maxlength": 512
		},
		"trip_goods": {
			"title": "出差物品",
			"notNull": true,
			"dataType": "varchar",
			"maxlength": 512
